{
  "background": {
    "scripts": [
      "js/jquery-1.11.0.min.js",
      "js/common.js"
    ]
  },
  "browser_action": {
    "default_icon": "image/icons/logo.png",
    "default_popup": "content/views/popup.html",
    "default_title": "ARTICLE"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "css/article_cj_comand.css"
      ],
      "js": [
        "js/jquery-1.11.0.min.js",
        "js/common.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "description": "用于对喜爱的文章进行收藏,以备你之后阅读、管理和分享。",
  "icons": {
    "16": "image/icons/icon16.png",
    "19": "image/icons/icon24.png",
    "48": "image/icons/logo.png"
  },
  "key": "",
  "manifest_version": 2,
  "minimum_chrome_version": "6.0.0.0",
  "name": "ARTICLE",
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "*://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.9",
  "content_security_policy": "script-src 'self' https://127.0.0.1:3000; object-src 'self'",
  "web_accessible_resources": [
    "index.html",
    "image/icons/icon_3.png",
    "image/icons/discover.png",
    "image/icons/icon1.png"
  ]
}
